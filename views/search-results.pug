div.searchHeader Users
div.matchingUsers
if matchingUsers
    each user in matchingUsers
        div.searchUser
            if user.background
                img.searchUser_pic(src=user.background title=`${user.username}` class='ajax-link')
                if (user.longUserName)
                    div.searchUser_username.tooLong(title=`${user.username}` class='ajax-link')=user.username
                else
                    div.searchUser_username(title=`${user.username}` class='ajax-link')=user.username
            else
                img.searchUser_pic(src='/images/vaporwave.jpg' title=`${user.username}` class='ajax-link')
                div.searchUser_username(title=`${user.username}` class='ajax-link')=user.username
else
    div No users matching your search

div.searchHeader Songs
if matchingSongs
div.matchingSongs
    each song in matchingSongs
        div.searchSong
            if song.pic
                div.searchSong_pic.playSong(style=`background-image: url(${song.pic})` id=song.music title=song.title)
            else
                div.searchSong_pic.playSong(style=`background-image: url(/images/missing_profile.png)` id=song.music title=song.title)
            div.searchSong_details(title=`${song.User.username}/${song.songLocalPath}` class='ajax-link') Title: #{song.title}
            div.searchSong_details.searchSong_artist(title=`${song.User.username}` class='ajax-link') Artist: #{song.artist}
            //- div.searchSong_details Album: #{song.album}
            //- div.searchSong_details Genre: #{song.genre}
