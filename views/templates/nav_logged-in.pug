extends footer
block navigation
    script
        include ../../scripts/logout.js
    nav(class="navbar sticky")
        ul(class="navbar__col navbar__col-1")
            li
                img(class="logo" src="header-logo.jpg")
            li
                a(href="/explore") Explore
        script
            include ../../scripts/search.js
        form(class="navbar__col navbar__col-" id="searchbar" method="get" action="/search")
            label(for="search") Search:
            input(type="search" id="search" name="search")
            input(type="submit" value="Search")
        ul(class="navbar__col navbar__col-3")
            li
                a(href="/explore") Upload
            li
                a.user(href="/explore") User
            li
                a(href="/explore") ...
            li: a.logout Log Out?
